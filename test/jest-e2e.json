{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "davilabslibs/web3/(.*)": "<rootDir>/../libs/web3/src/$1",
    "davilabslibs/web3": "<rootDir>/../libs/web3/src"
  }
}